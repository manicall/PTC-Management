<ResourceDictionary 
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"  
  xmlns:views = "clr-namespace:PTC_Management.Views"  
  xmlns:dialogViewModels = "clr-namespace:PTC_Management.ViewModel"  
  xmlns:viewModels = "clr-namespace:PTC_Management.ViewModel" 
          >
  <DataTemplate DataType="{x:Type dialogViewModels:EmployeeDialogViewModel}">
    <views:EmployeeDialogView />
  </DataTemplate>

  <DataTemplate DataType="{x:Type dialogViewModels:TransportDialogViewModel}">
    <views:TransportDialogView />
  </DataTemplate>

  <DataTemplate DataType="{x:Type dialogViewModels:RouteDialogViewModel}">
    <views:RouteDialogView />
  </DataTemplate>

  <DataTemplate DataType="{x:Type dialogViewModels:ItineraryDialogViewModel}">
    <views:ItineraryDialogView />
  </DataTemplate>

  <DataTemplate DataType="{x:Type dialogViewModels:MaintanceLogDialogViewModel}">
    <views:MaintanceLogDialogView />
  </DataTemplate>

  <DataTemplate DataType="{x:Type dialogViewModels:LogOfDepartureAndEntryDialogViewModel}">
    <views:LogOfDepartureAndEntryDialogView />
  </DataTemplate>

</ResourceDictionary>